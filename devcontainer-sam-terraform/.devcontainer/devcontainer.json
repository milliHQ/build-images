{
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:14-bullseye",
  "features": {
    "common": {
      "username": "automatic",
      "uid": "automatic",
      "gid": "automatic",
      "installZsh": true,
      "installOhMyZsh": true,
      "upgradePackages": true,
      "nonFreePackages": false
    },
    "homebrew": "latest",
    "milliHQ/dev-container-features-homebrew-utils/homebrew-install": {
      "tap": "aws/tap",
      "packages": "aws-sam-cli"
    },
    "terraform": {
      "version": "latest",
      "tflint": "none",
      "terragrunt": "none"
    },
    "docker-in-docker": "latest"
  }
}
